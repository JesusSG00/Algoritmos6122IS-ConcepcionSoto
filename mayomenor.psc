Algoritmo detarea
	
	Definir a,b,c Como real;
	
	Escribir "Ingresa tres numeros";
	
	leer a,b,c;
	
	si a > b y b>c  Entonces
		Escribir "LOS NUMEROS SON:",a,b,c;
	FinSi

			si b>a y a>c Entonces
				Escribir "LOS NUMEROS SON: ",b,a,c;
			FinSi
		
			
			si c>a y a<b Entonces
				Escribir "LOS NUMEROS SON: ",c,b,a;
			FinSi
			
				Si c>b y a>b Entonces
					Escribir "LOS NUMEROS SON: ",c,a,b;
				FinSi


FinAlgoritmo